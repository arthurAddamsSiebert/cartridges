			</tr>
			<tr>
				<td class="table_detail center s e"><input type="checkbox" name="LockedMonitorsInfo" value="true" <isif condition="#LockedMonitors EQ 'true'#">checked="checked"</isif>/></td>
				<td class="table_detail s e"><istext key="ThreaddumpProperties.LockedMonitors.table_detail"/></td>
				<td class="table_title_description s"><istext key="ThreaddumpProperties.InformationAboutLockedMonitors.table_title_description"/></td>
			</tr>			
			<tr>
				<td class="table_detail center s e"><input type="checkbox" name="LockedSynchronizersInfo" value="true" <isif condition="#LockedSynchronizers EQ 'true'#">checked="checked"</isif>/></td>
				<td class="table_detail s e"><istext key="ThreaddumpProperties.LockedSynchronizers.table_detail"/></td>
				<td class="table_title_description s"><istext key="ThreaddumpProperties.InformationAboutLockedSynchronizers.table_title_description"/></td>
			</tr>	
