<iscontent type="text/html" charset="UTF-8" compact="true">
<tr>
	<td class="form_bg w e s">
		<table cellspacing="0" cellpadding="0" border="0" width="100%">
			<tr>
				<td class="table_header e s" nowrap="nowrap">
					<div id="B" <isif condition="#(ApplicationInformation EQ 'false') OR (EnfinityVersion EQ 'false') OR (OperatingSystem EQ 'false') OR (JavaVersion EQ 'false') OR (LoadedCartridges EQ 'false') OR (Datasources EQ 'false') OR (Drivers EQ 'false') OR (LicenseInformation EQ 'false')#">style="display:none"</isif>>
