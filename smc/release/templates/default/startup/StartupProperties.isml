							<tr>
								<td nowrap="nowrap"><a href="javascript:selectAll('StartUp');" class="tableheader"><istext key="StartupProperties.SelectAll.link"/></a></td>
							</tr>
						</table>
					</div>
				</td>
				<td class="table_header s e" width = "40%"><istext key="StartupProperties.Name.table_header"/></td>
