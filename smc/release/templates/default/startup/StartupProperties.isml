							<tr>
								<td nowrap="nowrap"><a href="javascript:selectAll('StartUp');" class="tableheader"><istext key="StartupProperties.ClearAll.link"/></a></td>
							</tr>
						</table>
					</div>
					<div id="A" <isif condition="#(ApplicationInformation EQ 'true') AND (EnfinityVersion EQ 'true') AND (OperatingSystem EQ 'true') AND (JavaVersion EQ 'true') AND (LoadedCartridges EQ 'true') AND (Datasources EQ 'true') AND (Drivers EQ 'true') AND (LicenseInformation EQ 'true')#">style="display:none"</isif>>
