				<td class="table_title_description s"><istext key="StartupProperties.NameIPPortServergroups.table_title_description"/></td>
			</tr>
			<tr>
				<td class="table_detail center s e"><input type="checkbox" name="VersionNumber" value="true" <isif condition="#EnfinityVersion EQ 'true'#">checked="checked"</isif>/></td>
