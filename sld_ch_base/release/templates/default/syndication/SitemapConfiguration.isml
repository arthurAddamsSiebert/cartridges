<!--- TEMPLATENAME: SitemapConfiguration.isml --->
<iscontent charset="UTF-8">
<isset name="Syndication" value="#targetpageletviewpipelineparams:Syndication#" scope="request">
<isset name="SyndicationUUID" value="#targetpageletviewpipelineparams:SyndicationUUID#" scope="request">
<isset name="SiteMapWebForm" value="#targetpageletviewpipelineparams:SiteMapWebForm#" scope="request">
<isset name="Locales" value="#targetpageletviewpipelineparams:Locales#" scope="request">
<isset name="Currencies" value="#targetpageletviewpipelineparams:Currencies#" scope="request">
<isset name="SiteMapImagesForm" value="#targetpageletviewpipelineparams:SiteMapImagesForm#" scope="request">
<isset name="ImageViews" value="#targetpageletviewpipelineparams:ImageViews#" scope="request">
<isset name="ImageTypeViewPairs" value="#targetpageletviewpipelineparams:ImageTypeViewPairs#" scope="request">
<isset name="DeleteConfirmation" value="#targetpageletviewpipelineparams:DeleteConfirmation#" scope="request">
<isset name="ImagePosition" value="#targetpageletviewpipelineparams:ImagePosition#" scope="request">
<isset name="DuplicatePair" value="#targetpageletviewpipelineparams:DuplicatePair#" scope="request">
<isset name="MissingImageType" value="#targetpageletviewpipelineparams:MissingImageType#" scope="request">
<isset name="NoSelectedImagesError" value="#targetpageletviewpipelineparams:NoSelectedImagesError#" scope="request">
<isset name="Clipboard" value="#targetpageletviewpipelineparams:Clipboard#" scope="request">
<isset name="IncludeProductsChecked" value="#targetpageletviewpipelineparams:IncludeProductsChecked#" scope="request">
<isset name="IncludeCategoriesChecked" value="#targetpageletviewpipelineparams:IncludeCategoriesChecked#" scope="request">
<isset name="IncludeStaticPagesChecked" value="#targetpageletviewpipelineparams:IncludeStaticPagesChecked#" scope="request">
<isset name="URLRewriteEnabled" value="#targetpageletviewpipelineparams:URLRewriteEnabled#" scope="request">
<isset name="FormSubmitted" value="#targetpageletviewpipelineparams:FormSubmitted#" scope="request">

<ISCreateMap mapname="targetpageletviewpipelineparams" mode="add"
	key0="Syndication" value0="#null#"
	key1="SiteMapWebForm" value1="#null#"
	key2="Locales" value2="#null#"
	key3="Currencies" value3="#null#"
	key4="SiteMapImagesForm" value4="#null#"	
	key5="ImageViews" value5="#null#"
	key6="ImageTypeViewPairs" value6="#null#"
>
<ISCreateMap mapname="targetpageletviewpipelineparams" mode="add"
	key0="DeleteConfirmation" value0="#null#"
	key1="DuplicatePair" value1="#null#"
	key2="MissingImageType" value2="#null#"
	key3="NoSelectedImagesError" value3="#null#"
	key4="IncludeProductsChecked" value4="#null#"
	key5="IncludeCategoriesChecked" value5="#null#"
	key6="IncludeStaticPagesChecked" value6="#null#"
>

<table cellpadding="0" cellspacing="0" border="0" width="100%">
	<input type="hidden" name="FormSubmitted" value="<isprint value="#FormSubmitted#">"/>
	<tr>
		<td class="table_header w e s"><istext key="SitemapConfiguration.Parameters.table_header" encoding="off"/></td>
	</tr>
	<!-- start error handling -->
	<isif condition="#isDefined(FormSubmitted) AND (FormSubmitted NE '') AND SiteMapWebForm:Submitted and (SiteMapWebForm:FileName:isMissing or SiteMapWebForm:FileName:isInvalid)#">
		<tr>
			<td width="100%" colspan="2">
			<table border="0" cellspacing="0" cellpadding="4" width="100%"
				class="error_box w e s">
				<tr valign="top">
					<td class="error_icon top e"><img
						src="#WebRoot()#/images/error.gif" width="16" height="15" alt=""
						border="0" /></td>
					<td class="error top" width="100%"><istext key="SitemapConfiguration.TheTargetCouldNotBeUpdated.error" encoding="off"/><br/>
					<isif
						condition="#SiteMapWebForm:FileName:isMissing or SiteMapWebForm:FileName:isInvalid#">
							 <istext key="SitemapConfiguration.PleaseProvideAValidFileName.error" encoding="off"/></isif>
					</td>
				</tr>
			</table>
			</td>
		</tr>
	</isif>
</table>

<input type="hidden" name="reload" value="reload" />
<table cellpadding="0" cellspacing="0" border="0" width="100%">
	<tr>
		<td class="table_title2 w e s"><istext key="SitemapConfiguration.GlobalProperties.table_title2" encoding="off"/></td>
	</tr>
	<isif condition="#NOT('Enabled' EQ URLRewriteEnabled)#">
		<tr>
			<td width="100%" colspan="2">
				<table border="0" cellspacing="0" cellpadding="4" width="100%" class="error_box w e s">
					<tr valign="top">
						<td class="error_icon top e">
							<img src="#WebRoot()#/images/error.gif" width="16" height="15" alt="" border="0" />
						</td>
						<td class="error top" width="100%">
							<istext key="SitemapConfiguration.AttentionURLRewritingIsDisabledForThisChannel.error" encoding="off"/>
						</td>
					</tr>
				</table>
			</td>
		</tr>
	</isif>
	
	<!-- Error Handling -->
	<isinclude template="content/search/inc/PageletSearchMaskErrors">
	
	<tr>
		<td>
			<table cellpadding="0" cellspacing="0" border="0" width="100%">
				<tr>
					<isif condition="#isDefined(FormSubmitted) AND (FormSubmitted NE '') AND SiteMapWebForm:FileName:isMissing OR SiteMapWebForm:FileName:isInvalid#">
						<td class="fielditem2_error w e s" nowrap="nowrap"><istext key="SitemapConfiguration.FileName.fielditem2_error" encoding="off"/>:*</td>
					<iselse>
						<td class="fielditem2 w e s" nowrap="nowrap"><istext key="SitemapConfiguration.FileName.fielditem2" encoding="off"/>:<span class="star">*</span></td>
					</isif>
					<td class="fielditem2 e s" width="100%">
						<isif condition="#isDefined(SiteMapWebForm:FileName:Value)#">
							<input type="text" name="SiteMapWebForm_FileName" maxlength="255" size="50" value="<isprint value="#SiteMapWebForm:FileName:Value#">" class="inputfield_en"/>
						<iselse>
							<isif condition="#isDefined(PermissionMap:SLD_SYNDICATE_CATALOGS)#">
								<input type="text" name="SiteMapWebForm_FileName" maxlength="255" size="50" value="<isprint value="#Syndication:FileName#">" class="inputfield_en" />
							<iselse>
								<input type="text" name="FileName-disabled" maxlength="255" size="50" value="<isprint value="#Syndication:FileName#">" class="inputfield_en" disabled="disabled" />
							</isif>	
						</isif>
					</td>
				</tr>
				<tr>
					<td class="fielditem2 w e s" nowrap="nowrap"><istext key="SitemapConfiguration.Locale.fielditem2" encoding="off"/>:</td>
					<td class="fielditem2 e s">		
						<isif condition="#isDefined(PermissionMap:SLD_SYNDICATE_CATALOGS)#">
							<isif condition="#isDefined(SiteMapWebForm:LocaleID:Value)#">
								<isset name="SelectedLocaleID" value="#SiteMapWebForm:LocaleID:Value#" scope="request" />
							<iselseif condition="#isDefined(Syndication:LocaleID)#">
								<isset name="SelectedLocaleID" value="#Syndication:LocaleID#" scope="request" />
							<iselse>
								<isset name="SelectedLocaleID" value="#CurrentSession:Locale:LocaleID#" scope="request" />
							</isif>
							<select name="SiteMapWebForm_LocaleID" class="select inputfield_en">
								<isloop iterator="Locales">
									<option value="<isprint value="#Locales:LocaleID#">"
										<isif condition="#Locales:LocaleID eq SelectedLocaleID#">selected="selected"</isif>>
											<isprint value="#Locales:getDisplayName(CurrentSession:Locale)#">&nbsp;
									</option>
								</isloop>
						<iselse>
							<select name="LocaleID-disabled" class="select inputfield_en" disabled="disabled">
							<isif condition="#isDefined(Syndication:LocaleID)#">
								<isloop iterator="Locales">
									<isif condition="#Locales:LocaleID EQ Syndication:LocaleID#">
										<option value="" selected="selected"><isprint value="#Locales:DisplayName(CurrentRequest:Locale)#"/></option>
										<isbreak/>
									</isif>
								</isloop>
							<iselse>
								<option value="" selected="selected"><istext key="SitemapConfiguration.NotSet.option" encoding="off"/></option>
							</isif>
						</isif>
						</select>
					</td>
				</tr>
				<tr>
					<td class="fielditem2 w e s" nowrap="nowrap"><istext key="SitemapConfiguration.Currency.fielditem2" encoding="off"/>:</td>
					<td class="fielditem2 e s" width="100%">
						<isif condition="#isDefined(PermissionMap:SLD_SYNDICATE_CATALOGS)#">
							<isif condition="#isDefined(SiteMapWebForm:CurrencyMnemonic:Value)#">
								<isset name="SelectedCurrency" value="#SiteMapWebForm:CurrencyMnemonic:Value#" scope="request" />
							<iselseif condition="#isDefined(Syndication:CurrencyMnemonic)#">
								<isset name="SelectedCurrency" value="#Syndication:CurrencyMnemonic#" scope="request" />
							<iselse>
								<isset name="SelectedCurrency" value="#CurrentSession:Currency:Mnemonic#" scope="request" />
							</isif>
						
							<select name="SiteMapWebForm_CurrencyMnemonic" class="select inputfield_en">
							<isloop iterator="Currencies">
								<option value="<isprint value="#Currencies:Mnemonic#">"
									<isif condition="#Currencies:Mnemonic eq SelectedCurrency#">selected="selected" </isif>>
									<isprint value="#Currencies:CurrencyName#">
								</option>
							</isloop>
						<iselse>
							<select name="CurrencyMnemonic-disabled" class="select inputfield_en" disabled="disabled">
							<isif condition="#isDefined(Syndication:CurrencyMnemonic)#">
								<isloop iterator="Currencies">
									<isif condition="#Currencies:Mnemonic EQ Syndication:CurrencyMnemonic#">
										<option value="" selected="selected"><isprint value="#Currencies:CurrencyName#"/></option>
										<isbreak/>
									</isif>
								</isloop>
							<iselse>
								<option value="" selected="selected"><istext key="SitemapConfiguration.NotSet.option" encoding="off"/></option>
							</isif>
						</isif>
						</select>
					</td>
				</tr>
				<tr>
					<td class="fielditem2 w e s" nowrap="nowrap"><istext key="SitemapConfiguration.CompressionLevel.fielditem2" encoding="off"/>:</td>
					<td class="fielditem2 e s" width="100%">
						<isif condition="#isDefined(PermissionMap:SLD_SYNDICATE_CATALOGS)#">
						
							<isif condition="#isDefined(SiteMapWebForm:CompressionLevel:Value)#">
								<isset name="SelectedCompressionLevel" value="#SiteMapWebForm:CompressionLevel:Value#" scope="request">
							<iselse>
								<isset name="SelectedCompressionLevel" value="#Syndication:CompressionLevel#" scope="request">
							</isif>
						
							<select	name="SiteMapWebForm_CompressionLevel" class="select inputfield_en">
								<option value="0" <isif condition="#SelectedCompressionLevel eq '0'#">selected="selected"</isif>>0</option>
								<option value="1" <isif condition="#SelectedCompressionLevel eq '1'#">selected="selected"</isif>>1</option>
								<option value="2" <isif condition="#SelectedCompressionLevel eq '2'#">selected="selected"</isif>>2</option>
								<option value="3" <isif condition="#SelectedCompressionLevel eq '3'#">selected="selected"</isif>>3</option>
								<option value="4" <isif condition="#SelectedCompressionLevel eq '4'#">selected="selected"</isif>>4</option>
								<option value="5" <isif condition="#SelectedCompressionLevel eq '5'#">selected="selected"</isif>>5</option>
								<option value="6" <isif condition="#SelectedCompressionLevel eq '6'#">selected="selected"</isif>>6</option>
								<option value="7" <isif condition="#SelectedCompressionLevel eq '7'#">selected="selected"</isif>>7</option>
								<option value="8" <isif condition="#SelectedCompressionLevel eq '8'#">selected="selected"</isif>>8</option>
								<option value="9" <isif condition="#SelectedCompressionLevel eq '9'#">selected="selected"</isif>>9</option>
						<iselse>
							<select	name="CompressionLevel-disabled" class="select inputfield_en" disabled="disabled">
								<isif condition="#isDefined(Syndication:CompressionLevel)#">
									<option selected="selected"><isprint value="#Syndication:CompressionLevel#"/></option>
								<iselse>
									<option value="" selected="selected"><istext key="SitemapConfiguration.NotSet.option" encoding="off"/></option>
								</isif>
						</isif>
						</select>
					</td>
				</tr>
				<tr>
					<td class="fielditem3 w e s" nowrap="nowrap"><istext key="SitemapConfiguration.PingService.fielditem3" encoding="off"/>:</td>
					<td class="e s" width="100%">
						<isif condition="#isDefined(PermissionMap:SLD_SYNDICATE_CATALOGS)#">
							<isif condition="#isDefined(SiteMapWebForm:PingSearchEngines:Value)#">
								<isset name="SelectedPingSearchEngines" value="#SiteMapWebForm:PingSearchEngines:Value EQ 'on'#" scope="request">
							<iselse>
								<isset name="SelectedPingSearchEngines" value="#Syndication:PingSearchEngines#" scope="request">
							</isif>
							<input type="checkbox" name="SiteMapWebForm_PingSearchEngines" id="pingSearchEnginesCheckbox" class="fielditem3 middle" <isif condition="#SelectedPingSearchEngines EQ 'true'#"> checked="checked" </isif> />
						<iselse>
							<input type="checkbox" name="PingSearchEngines-disable" id="pingSearchEnginesCheckbox" class="fielditem3 middle" <isif condition="#Syndication:PingSearchEngines EQ 'true'#"> checked="checked" </isif> disabled="disabled"/>
						</isif>
						<span class="fielditem3 middle"> <istext key="SitemapConfiguration.AutomaticallyPingSearchEngineWhenANewSitemapGenerated.fielditem3" encoding="off"/></span>
					</td>
				</tr>
			</table>
		</td>
	</tr>

	<tr>
		<td class="table_title2 middle w e s">
			<isif condition="#isDefined(PermissionMap:SLD_SYNDICATE_CATALOGS)#">
				<input type="checkbox" name="SiteMapWebForm_IncludeProducts" id="includeProductsCheckbox" class="fielditem2" onclick="this.form.submit()" <isif condition="#IncludeProductsChecked EQ 'true'#"> checked="checked" </isif> />
			<iselse>
				<input type="checkbox" name="IncludeProducts-disabled" id="includeProductsCheckbox" class="fielditem2" <isif condition="#IncludeProductsChecked EQ 'true'#"> checked="checked" </isif> disabled="disabled" />				
			</isif>
			<span class="middle"> <istext key="SitemapConfiguration.Products.fielditem2" encoding="off"/> </span>
		</td>
	</tr>

	<tr>
		<td>
			<table cellpadding="0" cellspacing="0" border="0" width="100%" id="productsPropertiesSection">
				<tr>
					<td class="fielditem2 w e s" nowrap="nowrap"><istext key="SitemapConfiguration.UseLastModifiedDate.fielditem2" encoding="off"/>: </td>
					<td class="fielditem2 e s" width="100%">
						<isif condition="#isDefined(PermissionMap:SLD_SYNDICATE_CATALOGS)#">
							<isif condition="#isDefined(SiteMapWebForm:UseLastModifiedProduct:Value)#">
								<isset name="SelectedUseLastModifiedProduct" value="#SiteMapWebForm:UseLastModifiedProduct:Value#" scope="request" />
							<iselse>
								<isset name="SelectedUseLastModifiedProduct" value="#Syndication:UseLastModifiedProduct#" scope="request" />
							</isif>
							<select	name="SiteMapWebForm_UseLastModifiedProduct" class="select inputfield_en">
								<option value="yes"
									<isif condition="#SelectedUseLastModifiedProduct eq 'yes'#">selected="selected"</isif>><istext key="SitemapConfiguration.Yes.option" encoding="off"/></option>
								<option value="no"
									<isif condition="#SelectedUseLastModifiedProduct eq 'no'#">selected="selected"</isif>><istext key="SitemapConfiguration.No.option" encoding="off"/></option>
						<iselse>
							<select	name="UseLastModifiedProduct-disabled" class="select inputfield_en" disabled="disabled">
								<isif condition="#isDefined(Syndication:UseLastModifiedProduct) AND (Syndication:UseLastModifiedProduct NE '')#">
									<option><isprint value="#Syndication:UseLastModifiedProduct#"/></option>
								<iselse>
									<option value=""><istext key="SitemapConfiguration.NotSet.option" encoding="off"/></option>
								</isif>
						</isif>
						</select>
					</td>
				</tr>
				
				<tr>
					<isif condition="#isDefined(SiteMapWebForm:ChangeFrequencyProduct:Value)#">
						<isset name="SelectedChangeFrequencyProduct" value="#SiteMapWebForm:ChangeFrequencyProduct:Value#" scope="request" />
					<iselse>
						<isset name="SelectedChangeFrequencyProduct" value="#Syndication:ChangeFrequencyProduct#" scope="request" />
					</isif>
				
					<td class="fielditem2 w e s" nowrap="nowrap"><istext key="SitemapConfiguration.ChangeFrequency.fielditem2" encoding="off"/>:</td>
					<td class="fielditem2 e s" width="100%">
						<isif condition="#isDefined(PermissionMap:SLD_SYNDICATE_CATALOGS)#">
							<select	name="SiteMapWebForm_ChangeFrequencyProduct" class="select inputfield_en"	id="changeFrequencyProductSelect">
						<iselse>
							<select	name="ChangeFrequencyProduct-disabled" class="select inputfield_en" id="changeFrequencyProductSelect" disabled="disabled">
						</isif>
							<option value=""><istext key="SitemapConfiguration.None.option" encoding="off"/></option>
							<option value="always"
								<isif condition="#SelectedChangeFrequencyProduct eq 'always'#">selected="selected"</isif>><istext key="SitemapConfiguration.Always.option" encoding="off"/></option>
							<option value="hourly"
								<isif condition="#SelectedChangeFrequencyProduct eq 'hourly'#">selected="selected"</isif>><istext key="SitemapConfiguration.Hourly.option" encoding="off"/></option>
							<option value="daily"
								<isif condition="#SelectedChangeFrequencyProduct eq 'daily'#">selected="selected"</isif>><istext key="SitemapConfiguration.Daily.option" encoding="off"/></option>
							<option value="weekly"
								<isif condition="#SelectedChangeFrequencyProduct eq 'weekly'#">selected="selected"</isif>><istext key="SitemapConfiguration.Weekly.option" encoding="off"/></option>
							<option value="monthly"
								<isif condition="#SelectedChangeFrequencyProduct eq 'monthly'#">selected="selected"</isif>><istext key="SitemapConfiguration.Monthly.option" encoding="off"/></option>
							<option value="yearly"
								<isif condition="#SelectedChangeFrequencyProduct eq 'yearly'#">selected="selected"</isif>><istext key="SitemapConfiguration.Yearly.option" encoding="off"/></option>
							<option value="never"
								<isif condition="#SelectedChangeFrequencyProduct eq 'never'#">selected="selected"</isif>><istext key="SitemapConfiguration.Never.option" encoding="off"/></option>
						</select>
					</td>
				</tr>
				<tr>
					<td class="fielditem2 w e s" nowrap="nowrap"><istext key="SitemapConfiguration.Priority.fielditem2" encoding="off"/>:</td>
					<td class="fielditem2 e s" width="100%">
						<isif condition="#isDefined(PermissionMap:SLD_SYNDICATE_CATALOGS)#">
							<isif condition="#isDefined(SiteMapWebForm:PriorityProduct:Value)#">
								<isset name="SelectedPriorityProduct" value="#SiteMapWebForm:PriorityProduct:Value#" scope="request" />
							<iselse>
								<isset name="SelectedPriorityProduct" value="#Syndication:PriorityProduct#" scope="request">
							</isif>
							<select	name="SiteMapWebForm_PriorityProduct" class="select inputfield_en" id="priorityProductSelect">
								<option value=""><istext key="SitemapConfiguration.None.option" encoding="off"/></option>
								<option value="0.0"
									<isif condition="#SelectedPriorityProduct eq '0.0'#">selected="selected"</isif>>0.0</option>
								<option value="0.1"
									<isif condition="#SelectedPriorityProduct eq '0.1'#">selected="selected"</isif>>0.1</option>
								<option value="0.2"
									<isif condition="#SelectedPriorityProduct eq '0.2'#">selected="selected"</isif>>0.2</option>
								<option value="0.3"
									<isif condition="#SelectedPriorityProduct eq '0.3'#">selected="selected"</isif>>0.3</option>
								<option value="0.4"
									<isif condition="#SelectedPriorityProduct eq '0.4'#">selected="selected"</isif>>0.4</option>
								<option value="0.5"
									<isif condition="#SelectedPriorityProduct eq '0.5'#">selected="selected"</isif>>0.5</option>
								<option value="0.6"
									<isif condition="#SelectedPriorityProduct eq '0.6'#">selected="selected"</isif>>0.6</option>
								<option value="0.7"
									<isif condition="#SelectedPriorityProduct eq '0.7'#">selected="selected"</isif>>0.7</option>
								<option value="0.8"
									<isif condition="#SelectedPriorityProduct eq '0.8'#">selected="selected"</isif>>0.8</option>
								<option value="0.9"
									<isif condition="#SelectedPriorityProduct eq '0.9'#">selected="selected"</isif>>0.9</option>
								<option value="1.0"
									<isif condition="#SelectedPriorityProduct eq '1.0'#">selected="selected"</isif>>1.0</option>
						<iselse>
							<select	name="PriorityProduct-disabled" class="select inputfield_en" id="priorityProductSelect" disabled="disabled">
								<isif condition="#isDefined(Syndication:PriorityProduct) AND (Syndication:PriorityProduct NE '')#">
									<option value="" selected="selected"><isprint value="#Syndication:PriorityProduct#"/></option>
								<iselse>
									<option value="" selected="selected"><istext key="SitemapConfiguration.None.option" encoding="off"/></option>
								</isif>
						</isif>
							
						</select>
					</td>
				</tr>
				
				<tr>
					<td colspan="2">
						<table cellpadding="5" cellspacing="0" border="0" width="100%">
							<tr class="infobox">
								<td class="infobox_title w e s" nowrap="nowrap" colspan="7">
									<istext key="SitemapConfiguration.ProductPictures.infobox_title" encoding="off"/>
								</td>
							</tr>
							<isif condition="#IsDefined(DeleteConfirmation) AND (DeleteConfirmation NE '') AND isDefined(PermissionMap:SLD_SYNDICATE_CATALOGS)#">
								<tr>
									<td colspan="7" style="padding:0px">
										<isif condition="#DeleteConfirmation EQ 'NoSelectedImagesError'#">
											<table border="0" cellpadding="4" cellspacing="0" width="100%" class="confirm_box">
												<tr>
													<td class="error_icon w e s"><img src="#WebRoot()#/images/error.gif" width="16" height="15" alt="" border="0"/></td>
													<td class="confirm e s" width="100%">
														<istext key="SitemapConfiguration.YouHaveNotSelectedAnyProductPictures.confirm" encoding="off"/>
													</td>
												</tr>
											</table>
										<iselseif condition="#DeleteConfirmation EQ 'DeleteSingleImage'#">
											<table border="0" cellpadding="4" cellspacing="0" width="100%" class="confirm_box">
												<tr>
													<td class="error_icon w e s"><img src="#WebRoot()#/images/confirmation.gif" width="16" height="15" alt="" border="0"/></td>
													<td class="confirm e s" width="100%">
														<istext key="SitemapConfiguration.DoYouWantToRemoveProductPictureClickOK.confirm" encoding="off"/>
													</td>
													<td class="e s">
														<table border="0" cellspacing="0" cellpadding="0">
															<tr>
																<td class="button">
																	<input type="hidden" name="SyndicationUUID"	value="#encodeValue(SyndicationUUID)#" />
																	<input type="hidden" name="ImageToDeletePosition"	value="<isprint value="#ImagePosition#">" />
																	<input type="submit" name="ConfirmDeleteImage" value="#localizeText('SitemapConfiguration.OK.button')#" class="button"/>
																</td>
																<td><img src="#Webroot()#/images/space.gif" width="4" height="0" alt="" border="0"/></td>
																<td class="button"><input type="submit" name="reload" value="#localizeText('SitemapConfiguration.Cancel.button')#" class="button"/></td>
															</tr>
														</table>
													</td>
												</tr>
											</table>
										<iselse>
											<table border="0" cellpadding="4" cellspacing="0" width="100%" class="confirm_box">
												<tr>
													<td class="error_icon w e s"><img src="#WebRoot()#/images/confirmation.gif" width="16" height="15" alt="" border="0"/></td>
													<td class="confirm e s" width="100%">
														<istext key="SitemapConfiguration.DoYouWantToRemoveProductPicturesClickOK.confirm" encoding="off"/>
													</td>
													<td class="e s">
														<table border="0" cellspacing="0" cellpadding="0">
															<tr>
																<td class="button">
																	<input type="submit" name="ConfirmDelete" value="#localizeText('SitemapConfiguration.OK.button')#" class="button"/>
																</td>
