<?xml version="1.0" encoding="UTF-8"?>
<query description="finds a product attribute by its uuid">
<parameters>
	<parameter name="UUID" type="java.lang.String" optional="false" description="the uuid of the product attribute to find."/>
</parameters>
<processor name="ORM">
</processor>
<template type="objects">
	SELECT FROM productattribute  
	WHERE uuid=<template-variable value="UUID"/>
</template>
</query>
