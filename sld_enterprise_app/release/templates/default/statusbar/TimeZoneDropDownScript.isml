<isplacement placeholderid="JSWebControls">
$('<isprint value="#selector#"/>').on('dropdownchange', function(event, ui) {
	var form = $(this).parents('form');
	form.append('<input type="hidden" name="<isprint value="#params:StatusbarForm:Referer:QualifiedName#"/>" value="' + window.location.href + '" />');
	form.submit();
});
</isplacement>
