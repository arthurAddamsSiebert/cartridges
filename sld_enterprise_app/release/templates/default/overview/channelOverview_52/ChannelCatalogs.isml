							isDefined(CurrentChannelPermissionMap:SLD_VIEW_BATCH_PROCESSES)			OR
							isDefined(CurrentChannelPermissionMap:SLD_VIEW_CATALOGS)				OR
							isDefined(CurrentChannelPermissionMap:SLD_VIEW_IMPEX_CATALOGS_PRODUCTS)	OR
