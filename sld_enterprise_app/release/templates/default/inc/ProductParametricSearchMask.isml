<tr id="addAttributeNA" <isif condition="#ShowGeneralProductAttributes NE 'true'#">style="display:none;"</isif>>
	<td>&nbsp;</td>
	<td>
		<table border="0" cellspacing="1" cellpadding="0">
			<tr>
				<td class="button"><input type="button" name="addAttribute" id="addAttributeButtonNA" value="#localizeText('ContentProcessingSelectLabel.Add.button')#" class="button" />
				</td>
			</tr>
		</table>
	</td>
	<td colspan="3"></td>
</tr>
<tr <isif condition="#ShowGeneralProductAttributes NE 'true'#">style="display:none;"</isif>><td colspan="5"><img src="#WebRoot()#/images/space.gif" width="1" height="10" alt="" border="0"/></td></tr>
<tr <isif condition="#ShowGeneralProductAttributes NE 'true'#">style="display:none;"</isif>>
	<isif condition="#WFAdvancedSearch:CategoryReference:isInvalid#">
		<td nowrap="nowrap" class="label"><label class="label label_light label_indent label_error" for="CategoryName"><istext key="ProductParametricSearchMask.Category.label" encoding="off"/>:</label></td>
	<iselse>
		<td nowrap="nowrap" class="label"><label class="label label_light label_indent" for="CategoryName"><istext key="ProductParametricSearchMask.Category.label" encoding="off"/>:</label></td>
	</isif>
	<td colspan="4">
		<table border="0" cellspacing="0" cellpadding="0" width="100%">
			<tr>
				<td class="infobox_item_before_button" width="37%">
					<input type="hidden" name="WFAdvancedSearch_CategoryReference" value="<isprint value="#CatalogCategory:UUID#">"/>
					<isif condition="#isdefined(CatalogCategory:DisplayName)#">
						<input type="text" name="CategoryName" id="CategoryName" maxlength="255" value="<isprint value="#CatalogCategory:DisplayName#">" class="inputfield_en w100" readonly="readonly"/>
					<iselse>
						<input type="text" name="CategoryName" id="CategoryName" maxlength="255" value="<isprint value="#CatalogCategory:Name#">" class="inputfield_en w100" readonly="readonly"/>
					</isif>
				</td>
				<td width="1%">
					<table border="0" cellspacing="4" cellpadding="0">
						<tr>
							<td class="button">
							<isif condition="#isDefined(CatalogOwningDomainUUID) AND (CatalogOwningDomainUUID NE '')#">
								<input type="submit" name="selectCategory" value="#localizeText('SearchQueryEditor.Select.button')#" class="button" onClick="this.form.action='#URL(Action('ViewProductParametricSearch-SelectCatalog'), Parameter('DivideByProxyFlag', 'false'), Parameter('CatalogOwningDomainUUID', getValue(CatalogOwningDomainUUID)) )#'" />
							<iselse>
								<input type="submit" name="selectCategory" value="#localizeText('SearchQueryEditor.Select.button')#" class="button" onClick="this.form.action='#URL(Action('ViewProductParametricSearch-SelectCatalog'), Parameter('DivideByProxyFlag', 'false'))#'" />
							</isif>
							</td>
							<td class="button">
								<input type="submit" name="clearCategory" value="#localizeText('SearchQueryEditor.Clear.button')#" class="button" onClick="this.form.WFAdvancedSearch_CategoryReference.value=''; this.form.CategoryName.value=''; return false;" />
							</td>
						</tr>					
					</table>
				</td>
				<td width="1%"><img src="#WebRoot()#/images/space.gif" width="15" height="1" alt="" border="0"/></td>
				<td width="1%" class="input_checkbox">
					<input type="checkbox" name="WFAdvancedSearch_IncludeSubCategories" id="WFAdvancedSearch_IncludeSubCategories" value="true" <isif condition="#(WFAdvancedSearch_IncludeSubCategories EQ 'true') OR (NOT isDefined(WFAdvancedSearch)) OR (NOT WFAdvancedSearch:isSubmitted)#">checked="checked"</isif> />
				</td>
				<td class="label_checkbox_text">
					<label class="label label_checkbox_text label_light" for="WFAdvancedSearch_IncludeSubCategories"><istext key="ProductSetProducts.IncludingSubCategories.label" encoding="off"/></label>
				</td>
			</tr>
		</table>
	</td>
</tr>
<tr <isif condition="#ShowGeneralProductAttributes NE 'true'#">style="display:none;"</isif>>
	<isif condition="#isDefined(PriceFromError) OR WFAdvancedSearch:PriceFrom:isInvalid#">
		<td nowrap="nowrap" class="label"><label class="label label_light label_indent label_error" for="WFAdvancedSearch_PriceFrom"><istext key="ProductParametricSearchMask.PriceFrom.label" encoding="off"/>:</label></td>
	<iselse>
		<td nowrap="nowrap" class="label"><label class="label label_light label_indent" for="WFAdvancedSearch_PriceFrom"><istext key="ProductParametricSearchMask.PriceFrom.label" encoding="off"/>:</label></td>
	</isif>
	<td colspan="4">
		<input type="hidden" name="PriceAttributeName_1" value="value"/><input type="hidden" name="PriceOperator_1" value="GTE"/><input type="hidden" name="PriceLocation_1" value="NATIVE_ATTRIBUTE"/><input type="hidden" name="PriceType_1" value="Double"/><input type="hidden" name="PriceSubString_1" value="False"/>
		<input type="hidden" name="PriceAttributeName_2" value="value"/><input type="hidden" name="PriceOperator_2" value="LTE"/><input type="hidden" name="PriceLocation_2" value="NATIVE_ATTRIBUTE"/><input type="hidden" name="PriceType_2" value="Double"/><input type="hidden" name="PriceSubString_2" value="False"/>
		<table border="0" cellpadding="0" cellspacing="0">
			<tr>
				<td><input type="text" name="WFAdvancedSearch_PriceFrom" id="WFAdvancedSearch_PriceFrom" maxlength="10" size="12" value="<isprint value="#WFAdvancedSearch_PriceFrom#">" class="inputfield_en"/>&nbsp;&nbsp;&nbsp;</td>
					<isif condition="#isDefined(PriceToError) OR WFAdvancedSearch:PriceTo:isInvalid#">
						<td class="label"><label class="label label_light label_error" for="WFAdvancedSearch_PriceTo"><istext key="OrderExportConditions.To.label" encoding="off"/>:</label></td>
					<iselse>
						<td class="label"><label class="label label_light" for="WFAdvancedSearch_PriceTo"><istext key="OrderExportConditions.To.label" encoding="off"/>:</label></td>
					</isif>
				<td><input type="text" name="WFAdvancedSearch_PriceTo" id="WFAdvancedSearch_PriceTo" maxlength="10" size="12" value="<isprint value="#WFAdvancedSearch_PriceTo#">" class="inputfield_en"/>&nbsp;</td>
				<td>
					<isif condition="#(isDefined(WFAdvancedSearch_PriceCurrencyMnemonic))#">
						<isset scope="request" name="PriceCurrencyMnemonic" value="#WFAdvancedSearch_PriceCurrencyMnemonic#">
