	<iselse>
		<td nowrap="nowrap" class="label"><label class="label label_light label_indent" for="WFAdvancedSearch_ProductID"><istext key="sld_enterprise_app.ProductID.productdetail_item" encoding="off"/>:</label></td>
	</isif>
	<td>
		<isif condition="#(isDefined(WFAdvancedSearch) AND (WFAdvancedSearch:ProductID:Value NE ''))#">
			<input type="text" name="WFAdvancedSearch_ProductID" id="WFAdvancedSearch_ProductID" maxlength="255" value="<isprint value="#WFAdvancedSearch:ProductID:Value#">" class="inputfield_en w100"/>
