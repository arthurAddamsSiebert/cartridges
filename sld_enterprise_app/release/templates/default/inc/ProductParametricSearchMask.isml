	<tr <isif condition="#ShowGeneralProductAttributes NE 'true'#">style="display:none;"</isif>>
		<td nowrap="nowrap" class="label_select"><label class="label label_select label_light label_indent" for="WFAdvancedSearch_SharedFrom"><istext key="ProductParametricSearchMask.SharedFrom.label" encoding="off"/>:</label></td>
		<td>
			<select name="WFAdvancedSearch_SharedFrom" id="WFAdvancedSearch_SharedFrom" class="select inputfield_en w100" >
				<option value=""><istext key="ContentEntryPointDefinitionInclusionFilterMask.Any.option" encoding="off"/></option>
