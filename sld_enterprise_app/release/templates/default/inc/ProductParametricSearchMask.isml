	</isif>
	<td>
		<isif condition="#(isDefined(WFAdvancedSearch) AND (WFAdvancedSearch:ProductName:Value NE ''))#">
			<input type="text" name="WFAdvancedSearch_ProductName" id="WFAdvancedSearch_ProductName" maxlength="255" value="<isprint value="#WFAdvancedSearch:ProductName:Value#">" class="inputfield_en w100"/>
		<iselse>
			<input type="text" name="WFAdvancedSearch_ProductName" id="WFAdvancedSearch_ProductName" maxlength="255" value="<isprint value="#WFAdvancedSearch_ProductName#">" class="inputfield_en w100"/>
		</isif>
	</td>
	<isif condition="#isdefined(ERROR_Label)#">
