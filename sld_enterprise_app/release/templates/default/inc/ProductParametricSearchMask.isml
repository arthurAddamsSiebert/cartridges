	<td colspan="4">&nbsp;</td>
	<td align="right">
		<table border="0" cellspacing="2" cellpadding="0">
			<tr>
				<td class="button"><input type="submit" name="findParametric" value="#localizeText('PageletEntryPointSearchMask.Find.button')#" class="button"/></td>
			</tr>
		</table>
		<!--- ISNOLOC --->
		<isplacement placeholderid="JSWebLibraries">ParametricSearchAttributes.js</isplacement>
		<!--- /ISNOLOC --->
		<script type="text/javascript" id="WFAdvancedSearch_Ref">
			var formName = $('#'#'#WFAdvancedSearch_Ref').closest('form').attr('name');

			<!--- determine how many custom attribute rows can be suppressed --->
			<isif     condition="#(WFAdvancedSearch_CustomAttributeName4 NE '')#">
				<isset name="rowCount" value="4" scope="request">
			<iselseif condition="#(WFAdvancedSearch_CustomAttributeName3 NE '')#">
				<isset name="rowCount" value="3" scope="request">
			<iselseif condition="#(WFAdvancedSearch_CustomAttributeName2 NE '')#">
				<isset name="rowCount" value="2" scope="request">
			<iselseif condition="#(WFAdvancedSearch_CustomAttributeName1 NE '')#">
				<isset name="rowCount" value="1" scope="request">
			<iselse>
				<isset name="rowCount" value="0" scope="request">
			</isif>
			searchAttributes(formName, '#'#'#addAttributeButton', '.js-row-custom-attribute', '.js-btn-delete-custom-attribute', <isprint value="#rowCount#" encoding="off">);

			<!--- determine how many native attribute rows can be suppressed --->
			<isif     condition="#(WFAdvancedSearch_NativeAttributeValue4 NE '') and (WFAdvancedSearch_NativeAttributeName4 NE '')#">
				<isset name="rowCount" value="4" scope="request">
			<iselseif condition="#(WFAdvancedSearch_NativeAttributeValue3 NE '') and (WFAdvancedSearch_NativeAttributeName3 NE '')#">
				<isset name="rowCount" value="3" scope="request">
			<iselseif condition="#(WFAdvancedSearch_NativeAttributeValue2 NE '') and (WFAdvancedSearch_NativeAttributeName2 NE '')#">
				<isset name="rowCount" value="2" scope="request">
			<iselseif condition="#(WFAdvancedSearch_NativeAttributeValue1 NE '') and (WFAdvancedSearch_NativeAttributeName1 NE '')#">
				<isset name="rowCount" value="1" scope="request">
			<iselse>
				<isset name="rowCount" value="0" scope="request">
			</isif>
			searchAttributes(formName, '#'#'#addAttributeButtonNA', '.js-row-native-attribute', '.js-btn-delete-native-attribute', <isprint value="#rowCount#" encoding="off">);
			
			function updatePriceUndefined() {
				var priceUndefined = $('#'#'#WFAdvancedSearch_PriceUndefined');
				var disabled = priceUndefined.prop('checked');
				$('#'#'#WFAdvancedSearch_PriceFrom').prop('disabled', disabled);
				$('#'#'#WFAdvancedSearch_PriceTo').prop('disabled', disabled);
			}
	
			updatePriceUndefined();			
		</script>	
	</td>
