<tr <isif condition="#ShowGeneralProductAttributes NE 'true'#">style="display:none;"</isif>>
	<isif condition="#WFAdvancedSearch:ManufacturerSKU:isInvalid#">
		<td nowrap="nowrap" class="label"><label class="label label_light label_indent label_error" for="WFAdvancedSearch_ManufacturerSKU"><istext key="LineItemBundledItem.ManufacturerSKU.fielditem2" encoding="off"/>:</label></td>
	<iselse>
		<td nowrap="nowrap" class="label"><label class="label label_light label_indent" for="WFAdvancedSearch_ManufacturerSKU"><istext key="LineItemBundledItem.ManufacturerSKU.fielditem2" encoding="off"/>:</label></td>
