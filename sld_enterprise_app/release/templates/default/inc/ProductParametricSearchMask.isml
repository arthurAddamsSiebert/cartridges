	<td nowrap="nowrap" class="label_select"><label class="label label_select label_light label_indent" for="WFAdvancedSearch_TypeCode"><istext key="ProductParametricSearchMask.ProductType.label" encoding="off"/>:</label></td>
	<td>
		<select name="WFAdvancedSearch_TypeCode" id="WFAdvancedSearch_TypeCode" class="select inputfield_en w100">
			<option value=""><istext key="ProductParametricSearchMask.AllTypes.option" encoding="off"/></option>
			<option value="typecombination_3" <isif condition="#WFAdvancedSearch_TypeCode EQ 'typecombination_3'#">selected="selected"</isif>> <istext key="BrowseClassificationCatalog.Product.table_detail" encoding="off"/> </option>			
			<option value="typecode_4" <isif condition="#WFAdvancedSearch_TypeCode EQ 'typecode_4'#">selected="selected"</isif>> <istext key="sld_enterprise_app.ProductBundle.table_detail" encoding="off"/> </option>
			<option value="typecode_8" <isif condition="#WFAdvancedSearch_TypeCode EQ 'typecode_8'#">selected="selected"</isif>> <istext key="sld_enterprise_app.BundledProduct.table_detail" encoding="off"/> </option>
			<option value="typecode_16" <isif condition="#WFAdvancedSearch_TypeCode EQ 'typecode_16'#">selected="selected"</isif>> <istext key="sld_enterprise_app.VariationMaster.table_detail" encoding="off"/> </option>
			<option value="typecode_32" <isif condition="#WFAdvancedSearch_TypeCode EQ 'typecode_32'#">selected="selected"</isif>> <istext key="sld_enterprise_app.VariationProduct.table_detail" encoding="off"/> </option>
			<option value="typecode_128" <isif condition="#WFAdvancedSearch_TypeCode EQ 'typecode_128'#">selected="selected"</isif>> <istext key="ProductBOTypeCode.RetailSet" encoding="off"/> </option>
			<option value="typecode_256" <isif condition="#WFAdvancedSearch_TypeCode EQ 'typecode_256'#">selected="selected"</isif>> <istext key="ProductBOTypeCode.PartOfRetailSet" encoding="off"/> </option>
			<option value="typecode_40" <isif condition="#WFAdvancedSearch_TypeCode EQ 'typecode_40'#">selected="selected"</isif>> <istext key="ProductParametricSearchMask.BundledAndVariationProduct.option" encoding="off"/> </option>
			<isif condition="#isDefined(ServiceTypeCategories) AND hasElements(ServiceTypeCategories)#">
				<isloop iterator="ServiceTypeCategories" alias="ServiceTypeCategory">
				    <isset name="servicetypevalue" value="#'servicetype_'.ServiceTypeCategory:UUID#" scope="request">
					<option value="<isprint value="#'servicetype_'.ServiceTypeCategory:UUID#">" <isif condition="#WFAdvancedSearch_TypeCode EQ servicetypevalue#">selected="selected"</isif>> <isprint value="#ServiceTypeCategory:DisplayName#"> </option>
				</isloop>
			</isif>
		</select>
	</td>
	<td>&nbsp;</td>
</tr>
<tr <isif condition="#ShowGeneralProductAttributes NE 'true'#">style="display:none;"</isif>>
