					<option value="<isprint value="#Domain:UUID#">" <isif condition="#WFAdvancedSearch_SharedFrom EQ Domain:UUID#">selected="selected"</isif>> <isprint value="#Domain:DomainName#"></option>
				</isloop>
			</select>
