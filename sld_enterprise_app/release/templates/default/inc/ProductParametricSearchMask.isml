					<select name="WFAdvancedSearch_PriceCurrencyMnemonic" class="select inputfield_en">
						<isloop iterator="Currencies">
							<option value="<isprint value="#Currencies:Mnemonic#">" <isif condition="#Currencies:Mnemonic EQ PriceCurrencyMnemonic#">selected="selected" </isif>><isprint value="#Currencies:CurrencyName#"></option>
						</isloop>
					</select>
				</td>
				<td><img src="#WebRoot()#/images/space.gif" width="15" height="1" alt="" border="0"/></td>
				<td class="input_checkbox">
					<input type="checkbox" name="WFAdvancedSearch_PriceUndefined" id="WFAdvancedSearch_PriceUndefined" value="true" <isif condition="#WFAdvancedSearch_PriceUndefined EQ 'true'#">checked="checked"</isif> onchange="javascript:updatePriceUndefined();" />
				</td>
				<td class="label_checkbox_text">
					<label class="label label_checkbox_text label_light" for="WFAdvancedSearch_PriceUndefined"><istext key="ProductClassificationSearchMask.Undefined.label" encoding="off"/></label>
				</td>
			</tr>
		</table>
	</td>
</tr>
