			document.getElementById('ConfigurationParameter_<isprint value="#ConfigurationParameterDefinition:Name#">_pull').value = 	values[4].split('=')[1];
		} catch (e) {}
		
		/*
		  function to save changed fields to the configuration parameter field
		*/
		function ConfigurationParameter_<isprint value="#ConfigurationParameterDefinition:Name#">_setValues() {
			var content="";
		
			content += 'width='	+	document.getElementById('ConfigurationParameter_<isprint value="#ConfigurationParameterDefinition:Name#">_width').value;
			content += ';prefix='+	document.getElementById('ConfigurationParameter_<isprint value="#ConfigurationParameterDefinition:Name#">_prefix').value;
			content += ';suffix='+	document.getElementById('ConfigurationParameter_<isprint value="#ConfigurationParameterDefinition:Name#">_suffix').value;
			content += ';push='+	document.getElementById('ConfigurationParameter_<isprint value="#ConfigurationParameterDefinition:Name#">_push').value;
			content += ';pull='+	document.getElementById('ConfigurationParameter_<isprint value="#ConfigurationParameterDefinition:Name#">_pull').value;
			
			document.getElementById('ConfigurationParameter_<isprint value="#ConfigurationParameterDefinition:Name#">').value = content;
		}
</script>
