<script type="text/javascript">
		var content= document.getElementById('ConfigurationParameter_<isprint value="#ConfigurationParameterDefinition:Name#">').value;
		var values= content.split(';');
		
		try {
			document.getElementById('ConfigurationParameter_<isprint value="#ConfigurationParameterDefinition:Name#">_width').value = 	values[0].split('=')[1];
			document.getElementById('ConfigurationParameter_<isprint value="#ConfigurationParameterDefinition:Name#">_prefix').value = 	values[1].split('=')[1];
