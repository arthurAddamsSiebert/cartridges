<iscontent type="text/html" charset="UTF-8" compact="true">

<isif condition="#(isDefined(previewAttribute)) AND (previewAttribute:ID EQ "CatalogCategoryOnlineIgnored") AND NOT(previewAttribute:Default)#">
	<div class="sfe-preview-summary-parameter-group-nondefault">
	    <isif condition="#previewAttribute:SelectedValue:ID EQ 'true'#">
		     <istext key="CatalogCategoryOnlineIgnored.AllCategoriesAreVisible"/>	     
		<iselse>
			 <istext key="CatalogCategoryOnlineIgnored.CategoryVisibilityIsNotOverridden"/>
	  	</isif>
  	</div>
</isif>