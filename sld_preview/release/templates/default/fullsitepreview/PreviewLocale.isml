<iscontent type="text/html" charset="UTF-8" compact="true">

<isif condition="#(isDefined(previewAttribute)) AND (previewAttribute:ID EQ "PreviewLocale")#">
	<tr>
			<td class="label"><istext key="PreviewLocale.Language.label"/>:</td>
			<td>
					<isprint value="#previewAttribute:SelectedValue:ValueObject:DisplayName(CurrentRequest:Session:Locale)#">
			</td>
	</tr>
</isif>

