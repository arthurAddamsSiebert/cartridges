<iscontent type="text/html" charset="UTF-8" compact="true">

<isif condition="#isDefined(previewConfigurationAttribute) AND isDefined(previewContext)#">
	<isset name="summaryTemplateToLoad" value="#'previewsummary/'.previewConfigurationAttribute:ID.'Summary.isml'#" scope="request">
	<isif condition="#existsTemplate(summaryTemplateToLoad)#">
		<isinclude template="#summaryTemplateToLoad#">
	</isif>
</isif>
