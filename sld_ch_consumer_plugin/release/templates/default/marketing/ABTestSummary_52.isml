                                    summarygroup    ="conversion"
                                    type            ="integer"
                                    isimportant     ="true">
                                    
            <%-- Test Object Viewed to Test Object Applied --%>
            <issummaryline      displaynamekey  ="TestObjectViewedEvent1.TestObjectAppliedEvent1"
                                statisticskey1  ="TestObjectViewedEvent-TestObjectAppliedEvent"
                                calculator1     ="chisqu"
                                    summarygroup    ="conversion"
                                    type            ="integer"
                                    isimportant     ="true">
            <%-- Test Object Viewed to Shopping Cart created --%>
            <issummaryline      displaynamekey  ="TestObjectViewedEvent1.ShoppingCartEvent1"
                                statisticskey1  ="TestObjectViewedEvent-ShoppingCartEvent"
                                calculator1     ="chisqu"
                                    summarygroup    ="conversion"
                                    type            ="integer"
                                                           >
            
            <%-- Test Object Clicked to Shopping Cart created --%>
            <issummaryline      displaynamekey  ="TestObjectClickedEvent1.ShoppingCartEvent1"
