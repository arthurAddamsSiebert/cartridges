<iscontent type="text/html" charset="UTF-8" compact="true">

<isinclude template="marketing/abtest/Modules">
<isinclude template="marketing/Modules">

<!-- Working Area -->
<!-- Main Content -->
<ISMarketingObjectDisplayName Object="#ABTest#" Locale="#CurrentSession:Locale#" Print="false" ObjectDisplayName="ABTestDisplayName">
<ISBreadcrumbTrail link="#URL(Action('ViewABTestSummary_52-Show'), Parameter('ChannelID', CurrentChannel:UUID), Parameter('ABTestUUID', ABTest:UUID))#" id="#ABTest:UUID#" text="#ABTestDisplayName#" text1="#' - '.localizeText('ABTestSummary_52.Summary.text1')#">

<!-- Tabs -->
<isput name="SelectedTab" value="Summary">
<isinclude template="marketing/ABTestTabs_52">
<!-- EO Tabs -->

<div class="table_title w e s" style="line-height: 34px">
	<isprint value="#ABTestDisplayName#"/>
	<isif condition="#ABTest:ContentApproved#">&nbsp;-&nbsp;<isABTestApprovalMessage abtest="ABTest"></isif>
</div>
<!-- keep content confirmation-->
<isif condition="#isDefined(keepContent)#">
<table border="0" cellpadding="0" cellspacing="0" width="100%">
	<tr>
		<td>
		
		<isform name="keepContentForm" action="#URL(Action('ViewABTestSetContentOnline_52-Dispatch'))#" method="post">
			<table border="0" cellpadding="0" cellspacing="0" width="100%" class="confirm_box">
				<ISMessageBox subject="ABTestGroup" type="sdc"
					message="#localizeText('ABTestSummary_52.KeepContentConfirmation.message')#"
					okbtnname="confirmKeepContent" 
					cancelbtnname="cancel" 
					colspan="1"
					parametername1="ABTestGroupUUID"
					parametervalue1="#ABTestGroup:UUID#"
					parametername2="ABTestUUID"
					parametervalue2="#ABTest:UUID#"
					parametername3="ChannelID"
				    parametervalue4="#CurrentChannel:UUID#">
			</table>
		</isform>
		</td>
	</tr>
</table>
</isif>
<div class="table_title_description w e s">
    <istext key="ABTestSummary_52.TheseStatisticsAreUpdatedEveryTimeTheJobUpdateTrack.table_title_description" encoding="off"/>      
   	<isif condition="#ABTest:ContentApproved#">
        <iscomment>nothing to display</iscomment>
	<iselse>
		<istext key="ABTestSummary_52.FinishTheABTestByClickingSetContentOnline.table_title_description" encoding="off"/> 
    </isif>         
</div>

<isif condition="#hasElements(ABTestGroups)#">
	<%-- Section: Overview --%>
	<h2 class="section s e w opened" onclick="$('div[selector=Overview]').toggle( 0, function() {$('div[selector=Overview]').is(':visible') ? $(this).addClass('opened') : $(this).removeClass('opened') }.bind(this) )">
		<istext key="ABTestSummary_52.Overview.section" encoding="off"/>
	</h2>
	<div selector="Overview" class="section" style="display: block">
		<table class="abtest_zebra_table s">
			<tr>
				<th class="table_header s w e left"><istext key="ABTestSummary_52.Statistics.table_header" encoding="off"/></th>
				<th class="table_header s e right" style="width: 15%">
					<isprint value="#ABTest:ControlGroup:DisplayName#"/> (<isprint value="#ABTest:ControlGroup:Ratio/100#" style="PERCENTAGE"/>)
				</th>
				<isloop iterator="ABTestGroups" alias="TestGroup">
					<isif condition="#ABTest:ControlGroup:UUID NE TestGroup:UUID#">
						<th class="table_header s e right" style="width: 15%">
							<isprint value="#TestGroup:DisplayName#"/> (<isprint value="#TestGroup:Ratio/100#" style="PERCENTAGE"/>)
						</th>
					</isif>
				</isloop>
				<th class="table_header s e right" style="width: 15%"><istext key="ABTestSummary_52.Total.table_header" encoding="off"/></th>
			</tr>
	        <%-- Test Object Viewed (max 1 per Session) --%>
	        <issummaryline      displaynamekey  ="TestObjectViewedEvent1"
	                            statisticskey1  ="TestObjectViewedEvent"
	                            calculator1     ="count"
	                            summarygroup    ="total"
	                            type            ="integer"
	                            isimportant     ="true">
	        <%-- Test Object Applied --%>
	        <issummaryline      displaynamekey  ="TestObjectAppliedEvent1"
	                            statisticskey1  ="TestObjectAppliedEvent"
	                            calculator1     ="count"
	                            summarygroup    ="total"
	                            type            ="integer"
	                            isimportant     ="true">
	        
	        <%-- Test Object Clicked --%>
	        <issummaryline      displaynamekey  ="TestObjectClickedEvent1"
	                            statisticskey1  ="TestObjectClickedEvent"
	                            calculator1     ="count"
	                            summarygroup    ="total"
	                            type            ="integer"
	                            isimportant     ="true">
	
	        <%-- Orders --%>
	        <issummaryline      displaynamekey  ="OrderCreatedEvent1"
	                            statisticskey1  ="OrderCreatedEvent"
	                            calculator1     ="countandsum"
	                            summarygroup    ="total"
	                            type            ="integer"
	                            isimportant     ="true"
	                            togglefor		="SummaryCurrency">
	        <isloop iterator="Currencies" alias="Currency" counter="Counter">
	            <issummaryline  displaynamekey  ="OrderCreatedEvent1"
	                            statisticskey1  ="OrderCreatedEvent"
	                            calculator1     ="countandsum"
	                            summarygroup    ="total"
	                            type            ="integer"
	                            currency		="#Currency#"
	                            isimportant     ="true"
	                            selector		="SummaryCurrency">
	        </isloop>
			<%-- Last Statistic Update --%>
			<issummaryline displaynamekey  ="lastupdate1"
			               statisticskey1  ="null"
			               calculator1     ="time"
			               summarygroup    ="lastupdate"
			               type            ="timestamp"
			               isimportant     ="true">
		</table>
	</div>
	
	<%-- Section: Total Values --%>
	<h2 class="section  s e w" onclick="$('div[selector=TotalValues]').toggle( 0, function() {$('div[selector=TotalValues]').is(':visible') ? $(this).addClass('opened') : $(this).removeClass('opened') }.bind(this) )">
		<istext key="ABTestSummary_52.TotalValues.section" encoding="off"/>
	</h2>
	<div selector="TotalValues" class="section">
		<table class="abtest_zebra_table">
			<tr>
				<th class="table_header s e w left"><istext key="ABTestSummary_52.Statistics.table_header" encoding="off"/></th>
				<th class="table_header s e right" style="width: 15%">
					<isprint value="#ABTest:ControlGroup:DisplayName#"/> (<isprint value="#ABTest:ControlGroup:Ratio/100#" style="PERCENTAGE"/>)
				</th>
				<isloop iterator="ABTestGroups" alias="TestGroup">
					<isif condition="#ABTest:ControlGroup:UUID NE TestGroup:UUID#">
						<th class="table_header s e right" style="width: 15%">
							<isprint value="#TestGroup:DisplayName#"/> (<isprint value="#TestGroup:Ratio/100#" style="PERCENTAGE"/>)
						</th>
					</isif>
				</isloop>
				<th class=" table_header s e right" style="width: 15%"><istext key="ABTestSummary_52.Total.table_header" encoding="off"/></th>
			</tr>
		
            <%-- Sessions (Participants) --%>
            <issummaryline      displaynamekey  ="ABTestSessionCreatedEvent1"
                                statisticskey1  ="ABTestSessionCreatedEvent"
                                calculator1     ="count"
                                    summarygroup    ="total"
                                    type            ="integer"
                                    isimportant     ="true">
            <%-- Shopping Carts created --%>
            <issummaryline      displaynamekey  ="ShoppingCartEvent1"
                                statisticskey1  ="ShoppingCartEvent"
                                calculator1     ="count"
                                    summarygroup    ="total"
                                    type            ="integer"
                                    isimportant     ="true">
            <%-- Checkouts --%>
            <issummaryline      displaynamekey  ="CheckoutEvent1"
                                statisticskey1  ="CheckoutEvent"
                                calculator1     ="count"
                                    summarygroup    ="total"
                                    type            ="integer"
                                    isimportant     ="true">
            <%-- Orders --%>
            <issummaryline      displaynamekey  ="OrderCreatedEvent1"
                                statisticskey1  ="OrderCreatedEvent"
                                calculator1     ="countandsum"
                                summarygroup    ="total"
                                type            ="integer"
                                isimportant     ="true"
	                            togglefor		="TotalValuesOrders">
            <isloop iterator="Currencies" alias="Currency">
                <issummaryline  displaynamekey  ="OrderCreatedEvent1"
                                statisticskey1  ="OrderCreatedEvent"
                                calculator1     ="countandsum"
                                summarygroup    ="total"
                                type            ="integer"      
                                currency		="#Currency#"
                                selector			="TotalValuesOrders"
                                isimportant     ="true">
            </isloop>
            <%-- Items Ordered --%>
            <issummaryline      displaynamekey  ="ItemsOrderedEvent1"
                                statisticskey1  ="ItemsOrderedEvent"
                                statisticmarker1 ="Sum"
                                calculator1     ="sum"
                                summarygroup    ="total"
                                type            ="integer"
	                            togglefor		="TotalValuesItems">
            <isloop iterator="Currencies" alias="Currency">
                <issummaryline  displaynamekey  ="ItemsOrderedEvent1"
                                statisticskey1  ="ItemsOrderedEvent"
                                statisticmarker1 ="Sum"
                                calculator1     ="sum"
                                summarygroup    ="total"
                                type            ="integer"
                                selector		="TotalValuesItems"
                                currency		="#Currency#">
            </isloop>
            <%-- Orders Value --%>
            <isloop iterator="Currencies" alias="Currency">
                <issummaryline  displaynamekey  ="OrderCreatedEvent2"
                                statisticskey1  ="OrderCreatedEvent"                             
                                statisticmarker1="Sum"
                                calculator1     ="countandsum"
                                summarygroup    ="total"
                                type            ="currency"     
                                currency		="#Currency#"
                                selector		="TotalValuesItems"
                                isimportant     ="true">
            </isloop>
		</table>
	</div>


	<%-- Section: Conversion --%>
	<h2 class="section  s e w" onclick="$('div[selector=Conversion]').toggle( 0, function() {$('div[selector=Conversion]').is(':visible') ? $(this).addClass('opened') : $(this).removeClass('opened') }.bind(this) )">
