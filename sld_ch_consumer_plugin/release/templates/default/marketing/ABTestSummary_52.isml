                                    summarygroup    ="conversion"
                                    type            ="integer"
                                                           >
            <%-- Test Object Viewed to Orders --%>
            <issummaryline      displaynamekey  ="TestObjectViewedEvent1.OrderCreatedEvent1"
                                statisticskey1  ="TestObjectViewedEvent-OrderCreatedEvent"
                                calculator1     ="chisqu"
                                summarygroup    ="conversion"
                                type            ="integer"
                                isimportant     ="true"
	                            togglefor		="ConversionTestObjectViewed">
            <isloop iterator="Currencies" alias="Currency">
