	<%-- Section: Average Values --%>
	<h2 class="section  s e w" onclick="$('div[selector=AverageValues]').toggle( 0, function() {$('div[selector=AverageValues]').is(':visible') ? $(this).addClass('opened') : $(this).removeClass('opened') }.bind(this) )">
		<istext key="ABTestSummary_52.AverageValues.section" encoding="off"/>
	</h2>
	<div selector="AverageValues" class="section">
		<table class="abtest_zebra_table">
			<tr>
				<th class="table_header s e w left"><istext key="ABTestSummary_52.Statistics.table_header" encoding="off"/></th>
				<th class="table_header s e right" style="width: 15%">
					<isprint value="#ABTest:ControlGroup:DisplayName#"/> (<isprint value="#ABTest:ControlGroup:Ratio/100#" style="PERCENTAGE"/>)
				</th>
				<isloop iterator="ABTestGroups" alias="TestGroup">
					<isif condition="#ABTest:ControlGroup:UUID NE TestGroup:UUID#">
						<th class="table_header s e right" style="width: 15%">
							<isprint value="#TestGroup:DisplayName#"/> (<isprint value="#TestGroup:Ratio/100#" style="PERCENTAGE"/>)
						</th>
					</isif>
				</isloop>
				<th class="table_header s e right" style="width: 15%"><istext key="ABTestSummary_52.Total.table_header" encoding="off"/></th>
			</tr>
	
            <issummaryline      displaynamekey  ="TestObjectViewedEvent1.ABTestSessionCreatedEvent1"
                                statisticskey1  ="TestObjectViewedEvent"                                                      
                                statisticskey2	="ABTestSessionCreatedEvent"
                                calculator1     ="count"                                                                      
                                calculator2		="count"
                                summarygroup    ="averageinpercent"
                                type            ="percent"
                                isimportant     ="true">
            <%-- Ratio: Orders per Session (%) --%>
            <issummaryline      displaynamekey  ="OrderCreatedEvent1.ABTestSessionCreatedEvent1"
                                statisticskey1  ="OrderCreatedEvent"                                                          
                                statisticskey2	="ABTestSessionCreatedEvent"
                                calculator1     ="countandsum"                                                                
                                calculator2		="count"
                                summarygroup    ="averageinpercent"
                                type            ="percent"
                                isimportant     ="true">
            <%-- Ratio: Orders per Checkout (%) --%>
            <issummaryline      displaynamekey  ="OrderCreatedEvent1.CheckoutEvent1"
                                statisticskey1  ="OrderCreatedEvent"                                                          
                                statisticskey2	="CheckoutEvent"
                                calculator1     ="countandsum"                                                                
                                calculator2   	="count"
                                summarygroup    ="averageinpercent"
                                type            ="percent"
                                isimportant     ="true">
            <%-- Orders that viewed the Test Object (%) --%>
            <issummaryline      displaynamekey  ="TestObjectViewedEvent1_OrderCreatedEvent1.OrderCreatedEvent1"
                                statisticskey1  ="TestObjectViewedEvent-OrderCreatedEvent"                                    
                                statisticskey2  ="OrderCreatedEvent"
                                calculator1     ="chisqu"                                                                     
                                calculator2     ="countandsum"
                                summarygroup    ="averageinpercent"
                                type            ="percent"
                                isimportant     ="true"
                                togglefor		="AverageValuesTestObjectVisible">
            <isloop iterator="Currencies" alias="Currency">
                <issummaryline  displaynamekey  ="TestObjectViewedEvent1_OrderCreatedEvent1.OrderCreatedEvent1"
                                statisticskey1  ="TestObjectViewedEvent-OrderCreatedEvent"                                    
                                statisticskey2  ="OrderCreatedEvent"
                                calculator1     ="chisqu"                                                                     
                                calculator2     ="countandsum"
                                summarygroup    ="averageinpercent"
                                type            ="percent"
                                currency		="#Currency#"
                                isimportant     ="true"
                                selector			="AverageValuesTestObjectVisible">
                <%-- ... their average Value --%>
                <issummaryline  displaynamekey  ="TestObjectViewedEvent1_OrderCreatedEvent2.TestObjectViewedEvent1_OrderCreatedEvent1"
                                statisticskey1  ="TestObjectViewedEvent-OrderCreatedEvent"       
                                statisticmarker1="Sum"       
                                statisticskey2  ="TestObjectViewedEvent-OrderCreatedEvent"
                                calculator1     ="ttest"                                                                      
                                calculator2     ="ttest"
                                summarygroup    ="average"
                                type            ="currency"
                                currency		="#Currency#"
                                selector			="AverageValuesTestObjectVisible">
            </isloop>
            <%-- Orders that applied the Test Object (%) --%>
            <issummaryline      displaynamekey  ="TestObjectAppliedEvent1_OrderCreatedEvent1.OrderCreatedEvent1"
                                statisticskey1  ="TestObjectAppliedEvent-OrderCreatedEvent"                                   
                                statisticskey2  ="OrderCreatedEvent"
                                calculator1     ="chisqu"                                                                      
                                calculator2     ="countandsum"
                                summarygroup    ="averageinpercent"
                                type            ="percent"
                                isimportant     ="true"
                                togglefor		="AverageValuesOrdersApplied">
            <isloop iterator="Currencies" alias="Currency">
                <issummaryline  displaynamekey  ="TestObjectAppliedEvent1_OrderCreatedEvent1.OrderCreatedEvent1"
                                statisticskey1  ="TestObjectAppliedEvent-OrderCreatedEvent"                                   
                                statisticskey2  ="OrderCreatedEvent"
                                 calculator1    ="chisqu"                                                                     
                                 calculator2    ="countandsum"
                                 summarygroup   ="averageinpercent"
                                 type           ="percent"
                                 currency		="#Currency#"
                                 selector			="AverageValuesOrdersApplied"
                                 isimportant    ="true">
                <%-- ... their average Value --%>
                <issummaryline  displaynamekey  ="TestObjectAppliedEvent1_OrderCreatedEvent2.TestObjectAppliedEvent1_OrderCreatedEvent1"
                                statisticskey1  ="TestObjectAppliedEvent-OrderCreatedEvent"      
                                statisticmarker1="Sum"       
                                statisticskey2  ="TestObjectAppliedEvent-OrderCreatedEvent"
                                calculator1     ="ttest"                                                                      
                                calculator2     ="ttest"
                                summarygroup    ="average"
                                type            ="currency"     
                                currency		="#Currency#"
                                selector			="AverageValuesOrdersApplied">
            </isloop>
            
            <%-- Orders that clicked the Test Object (%) --%>
            <issummaryline      displaynamekey  ="TestObjectClickedEvent1_OrderCreatedEvent1.OrderCreatedEvent1"
                                statisticskey1  ="TestObjectClickedEvent-OrderCreatedEvent"                                   
                                statisticskey2 ="OrderCreatedEvent"
                                calculator1     ="chisqu"                                                                     
                                calculator2    ="countandsum"
                                summarygroup    ="averageinpercent"
                                type            ="percent"
                                isimportant=    "true"
                                togglefor		="AverageValuesOrdersClicked">
            <isloop iterator="Currencies" alias="Currency">
                <issummaryline  displaynamekey  ="TestObjectClickedEvent1_OrderCreatedEvent1.OrderCreatedEvent1"
                                statisticskey1  ="TestObjectClickedEvent-OrderCreatedEvent"                                   
                                statisticskey2  ="OrderCreatedEvent"
                                calculator1     ="chisqu"                                                                     
                                calculator2     ="countandsum"
                                summarygroup    ="averageinpercent"
                                type            ="percent"     
								currency		="#Currency#"
								selector			="AverageValuesOrdersClicked"
                                isimportant     ="true">
                <%-- ... their average Value --%>
                <issummaryline  displaynamekey  ="TestObjectClickedEvent1_OrderCreatedEvent2.TestObjectClickedEvent1_OrderCreatedEvent1"
                                statisticskey1  ="TestObjectClickedEvent-OrderCreatedEvent"      
                                statisticmarker1="Sum"      
                                statisticskey2  ="TestObjectClickedEvent-OrderCreatedEvent"
                                calculator1     ="ttest"                                                                      
                                calculator2     ="ttest"
                                summarygroup	="average"
                                type            ="currency"     
                                currency		="#Currency#"
								selector			="AverageValuesOrdersClicked">
            </isloop>
            
            <%-- Checkouts that viewed the Test Object (%) --%>
            <issummaryline      displaynamekey  ="TestObjectViewedEvent1_CheckoutEvent1.CheckoutEvent1"
                                statisticskey1  ="TestObjectViewedEvent-CheckoutEvent"                                        
                                statisticskey2 ="CheckoutEvent"
                                calculator1     ="chisqu"                                                                     
                                calculator2    ="count"
                                    summarygroup    ="averageinpercent"
                                    type            ="percent"
                                    isimportant     ="true">
            <%-- Checkouts that applied the Test Object (%) --%>
            <issummaryline      displaynamekey  ="TestObjectAppliedEvent1_CheckoutEvent1.CheckoutEvent1"
                                statisticskey1  ="TestObjectAppliedEvent-CheckoutEvent"                                       
                                statisticskey2 ="CheckoutEvent"
                                calculator1     ="chisqu"                                                                     
                                calculator2    ="count"
                                    summarygroup    ="averageinpercent"
                                    type            ="percent"
                                    isimportant     ="true">
            
            <%-- Checkouts that clicked the Test Object (%) --%>
            <issummaryline      displaynamekey  ="TestObjectClickedEvent1_CheckoutEvent1.CheckoutEvent1"
                                statisticskey1  ="TestObjectClickedEvent-CheckoutEvent"                                       
                                statisticskey2 ="CheckoutEvent"
                                calculator1     ="chisqu"                                                                     
                                calculator2    ="count"
                                    summarygroup    ="averageinpercent"
                                    type            ="percent"
                                    isimportant     ="true">
            
            <%-- Items per Order --%>
            <issummaryline      displaynamekey  ="ItemsOrderedEvent1.OrderCreatedEvent1"
                                statisticskey1  ="ItemsOrderedEvent"                                                          
                                statisticskey2  ="OrderCreatedEvent"
                                statisticmarker1="Sum"
                                calculator1     ="sum"                                                                        
                                calculator2     ="countandsum"
                                summarygroup    ="average"
                                type            ="decimal"
                                togglefor		="AverageValuesItemsPerOrder">
            <isloop iterator="Currencies" alias="Currency">
                <issummaryline  displaynamekey  ="ItemsOrderedEvent1.OrderCreatedEvent1"
                                statisticskey1  ="ItemsOrderedEvent"
                                statisticmarker1="Sum"                                                          
                                statisticskey2  ="OrderCreatedEvent"
                                calculator1     ="sum"                                                                        
                                calculator2     ="countandsum"
                                summarygroup    ="average"
                                type            ="decimal"      
                                currency		="#Currency#"
                                selector			="AverageValuesItemsPerOrder">
            </isloop>
            <%-- Average Order Value --%>
            <isloop iterator="Currencies" alias="Currency">
                <issummaryline  displaynamekey  ="OrderCreatedEvent2.OrderCreatedEvent1"
                                statisticskey1  ="OrderCreatedEvent"                             
                                statisticmarker1="Sum"       
                                statisticskey2  ="OrderCreatedEvent"
                                calculator1     ="countandsum"                                                                
                                calculator2     ="countandsum"
                                summarygroup    ="average"
                                type            ="currency"     
                                currency		="#Currency#"
                                selector			="AverageValuesItemsPerOrder"
                                isimportant     ="true">
            </isloop>
            <%-- Average Item Value --%>
            <isloop iterator="Currencies" alias="Currency">
                <issummaryline  displaynamekey  ="OrderCreatedEvent2.ItemsOrderedEvent1"
                                statisticskey1  ="OrderCreatedEvent"                             
                                statisticmarker1="Sum"       
                                statisticskey2  ="ItemsOrderedEvent"
                                statisticmarker2="Sum"
                                calculator1     ="countandsum"                                                                
                                calculator2     ="sum"
                                summarygroup    ="average"
                                type            ="currency"     
                                currency		="#Currency#"
                                selector			="AverageValuesItemsPerOrder">
            </isloop>
		</table>
	</div>
