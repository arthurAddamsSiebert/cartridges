            <%-- Test Object Clicked to Orders --%>
            <issummaryline      displaynamekey  ="TestObjectClickedEvent1.OrderCreatedEvent1"
                                statisticskey1  ="TestObjectClickedEvent-OrderCreatedEvent"
                                calculator1     ="chisqu"
                                summarygroup    ="conversion" 
                                type            ="integer"
                                isimportant     ="true"
	                            togglefor		="ConversionTestObjectClicked">
            <isloop iterator="Currencies" alias="Currency">
                <issummaryline  displaynamekey  ="TestObjectClickedEvent1.OrderCreatedEvent1"
                                statisticskey1  ="TestObjectClickedEvent-OrderCreatedEvent"
                                calculator1     ="chisqu"
