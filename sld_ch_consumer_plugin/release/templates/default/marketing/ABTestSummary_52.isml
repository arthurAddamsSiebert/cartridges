                                statisticskey1  ="TestObjectAppliedEvent-OrderCreatedEvent"
                                calculator1     ="chisqu"
                                summarygroup    ="conversion"
                                type            ="integer"
                                currency		="#Currency#"
                                selector			="ConversionTestObjectApplied"
                                isimportant     ="true">
            </isloop>
            <%-- Test Object Viewed to Items Ordered --%>
            <issummaryline      displaynamekey  ="TestObjectViewedEvent1.ItemsOrderedEvent1"
                                statisticskey1  ="TestObjectViewedEvent-ItemsOrderedEvent"       
                                statisticmarker1="Sum"
                                calculator1     ="ttest"
                                summarygroup    ="conversion"
                                type            ="integer"
	                            togglefor		="ConversionTestObjectOrdered">
            <isloop iterator="Currencies" alias="Currency">
