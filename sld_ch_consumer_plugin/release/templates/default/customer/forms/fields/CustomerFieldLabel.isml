	<label class="<isif condition="#isDefined(LabelClass) AND (LabelClass NE '')#"><isprint value="#LabelClass#"/><iselse>label</isif><isif condition="#Field:Invalid#"> label_error</isif>" for="<isprint value="#Field:QualifiedName#"/>">
		<istext key="#'customer.form.fields.labels.'. Field:ID#"/>
		<isif condition="#NOT Field:Field:Optional#"><span class="star">*</span></isif>
	</label>
