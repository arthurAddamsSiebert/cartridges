	<tr><td colspan="2"><img height="4" width="1" border="0" alt="" src="#WebRoot()#/images/space.gif" /></td></tr>
	<tr>
		<td class="label" nowrap="nowrap"><label class="label label_light"><istext key="OrderAddressInfo_52.Country.label" encoding="off"/>:</label></td>
		<td class="table_detail" width="100%"><isprint value="#address:Country#">&nbsp;</td>
	</tr>
	<isif condition="#isDefined(address:Extension("Store"))#">
