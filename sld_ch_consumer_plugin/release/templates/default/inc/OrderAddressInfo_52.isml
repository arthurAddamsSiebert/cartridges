			<td class="label" nowrap="nowrap"><label class="label label_light"><istext key="OrderAddressInfo_52.StoreID.label" encoding="off"/>:</label></td>
			<td class="table_detail"><isprint value="#address:AddressName#">&nbsp;</td>
		</tr>	
	<iselse>
