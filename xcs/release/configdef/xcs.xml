<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.intershop.com/xml/ns/enfinity/7.0/configdef">
	<group name="Inventory">
		<!-- The following parameter defines the choice of adapters. 
		 	 If an adapter should be added in another cartridge, there must be defined another configdef 
		 	 containing a parameter with the same name in same parameter group. Thus, it will be added -->
		<parameter name="adapter.selector.inventory" type="String" position="10"> 
			<labelKey>serviceparam.param.label.adapter.select</labelKey>
			<predefined position="10" value="simple">
				<valueKey>serviceparam.predef.label.inventoryservice.simple</valueKey>
			</predefined>
		</parameter>
	</group>
</definitions>