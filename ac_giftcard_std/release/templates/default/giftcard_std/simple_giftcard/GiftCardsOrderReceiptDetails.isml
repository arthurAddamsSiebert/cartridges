<iscontent type="text/html" charset="UTF-8">

<h5><istext key="checkout.giftcard.details.heading" /></h5>
<isloop iterator="GiftCardsDetail:ReceiptViewContent:Values" alias="GiftCard">
	<div data-testing-id="giftcardNumber">
		<istext key="checkout.giftcard.details.number" />
		<span><isprint value="#GiftCard:Cardnumber#"></span>
	</div>
	<p data-testing-id="giftcardPin">
		<istext key="checkout.giftcard.details.pin" />
		<span><isprint value="#GiftCard:Pin#"></span>
	</p>
</isloop>
