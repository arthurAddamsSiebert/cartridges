<iscontent charset="UTF-8">
<isif condition="#(ConditionForm:MinValue:Invalid eq 'true')#">
	<isif condition="#isdefined(ConditionForm:MinValue:Message)#">
		<istext key="#ConditionForm:MinValue:Message#"/>
	</isif>
</isif>
<isif condition="#(ConditionForm:MaxValue:Invalid eq 'true')#">
