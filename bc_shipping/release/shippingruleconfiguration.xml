	<functionconditions>
	     <functioncondition name="SizeCondition">
	             <returnType>boolean</returnType>
	             <functionBody>
if(product.getWidth()== null || product.getHeight() == null || product.getLength() == null) 
{ 
    return false; 
} 
return (product.getLength() + 2*( product.getWidth() + product.getHeight())) &gt; sizeCondition; 
                 </functionBody>
	             <parameters>
	                   <parameter>
	                         <parameterName>sizeCondition</parameterName>
	                         <parameterType>java.lang.Double</parameterType>
	                   </parameter>
	                   <parameter>
	                         <parameterName>product</parameterName>
	                         <parameterType>com.intershop.component.shipping.internal.ruleengineadapter.businessobject.Product</parameterType>
	                   </parameter>
	             </parameters>
	     </functioncondition>
	     <functioncondition name="VolumeCondition">
	             <returnType>boolean</returnType>
	             <functionBody>
if(product.getWidth()== null || product.getHeight() == null || product.getLength() == null) 
{ 
    return false; 
} 
return (product.getLength() * product.getHeight() * product.getWidth()) &gt; volumeCondition;
                 </functionBody>
	             <parameters>
	                   <parameter>
	                         <parameterName>volumeCondition</parameterName>
	                         <parameterType>java.lang.Double</parameterType>
	                   </parameter>
	                   <parameter>
	                         <parameterName>product</parameterName>
	                         <parameterType>com.intershop.component.shipping.internal.ruleengineadapter.businessobject.Product</parameterType>
	                   </parameter>
	             </parameters>	     
	     </functioncondition>
	</functionconditions>
